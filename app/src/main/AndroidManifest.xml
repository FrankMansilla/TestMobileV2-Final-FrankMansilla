<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_agregar_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_agregar_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crudlibro"
        tools:targetApi="31">

        <activity
            android:name=".EmbedVideoYoutube"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GestionarLibroActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ListadoLibrosActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ListadoLibros2Activity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BuscarLibroActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>